{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      "module": "CommonJS",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "strict": true,
      "outDir": "../dist/server"
    },
    "include": ["**/*.ts"]
  }