{
  "name": "api_server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"vite\" \"ts-node -r tsconfig-paths/register -P server/tsconfig.json server/server.ts\"",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "start": "ts-node server/server.ts",
    "start_server": "ts-node -r tsconfig-paths/register -P server/tsconfig.json server/server.ts",
    "serve": "npm run build && npm run start"
  },
  "dependencies": {
    "@types/multer": "^1.4.12",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "webdav": "^5.8.0",
    "webdav-server": "^2.6.2"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "npm-run-all2": "^7.0.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vue-tsc": "^2.2.0",
    "vuex": "^4.1.0"
  }
}
